{
  "name": "posts",
  "version": "0.1.0",
  "description": "The template project for [nano-react-app](https://github.com/adrianmcli/nano-react-app).",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "lint:js": "eslint src/**/*.js",
    "lint:css": "stylelint src/**/*.css"
  },
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "parcel-plugin-asset-copier": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "babel-preset-nano-react-app": "0.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-react": "7.17.0",
    "parcel-bundler": "1.12.4",
    "stylelint": "12.0.0",
    "stylelint-config-standard": "19.0.0"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liubich/posts.git"
  },
  "author": "liubich",
  "bugs": {
    "url": "https://github.com/liubich/posts/issues"
  },
  "homepage": "https://posts-react-spa.netlify.com",
  "assetsPath": "public"
}
